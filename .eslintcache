nent={ PieGalleryContainer } />\n          <PrivateRoute exact path=\"/pies/new\" component={ PieForm } />\n          <PrivateRoute exact path=\"/pies/:id\" component={ PieControllerWrapper } />\n        </Switch>\n      </Router>\n    </Provider>\n  )\n}\nexport default App;\n","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PieForm.js",["96"],"import React, { useState } from \"react\"\n\nimport PropTypes from 'prop-types'\nimport { connect } from 'react-redux'\nimport { createPie } from '../actions/pieActions'\n\nexport const PieForm = ({\n  createPie,\n  history,\n  user\n}) => {\n\n  const [pie, setPie] = useState({\n    title: '',\n    pieces: ['', '', '', '', '', '', '', '']\n  })\n  \n  const setTitle = (event) => {\n    setPie({\n      ...pie,\n      title: event.target.value\n    })\n  }\n\n  const setPiece = (event) => {\n    const p = [...pie.pieces]\n    p[event.target.id] = event.target.value\n    setPie({\n      ...pie,\n      pieces: p\n    })\n  }\n\n  String.prototype.isEmpty = function() {\n    return (this.length === 0 || !this.trim())\n  }\n\n  return (\n    <div>\n      <h1>Create Pie</h1>\n      <form className='form'>\n        <label htmlFor='title'>Title</label>\n        <input type='text' name='title' value={ pie.title } onChange={ setTitle } />\n\n        <label htmlFor='piece00'>Piece</label>\n        <input type='text' name='piece00' id={0} value={ pie.piece00 } onChange={ setPiece } />\n\n        <label htmlFor='piece01'>Piece</label>\n        <input type='text' name='piece01' id={1} value={ pie.piece01 } onChange={ setPiece }/>\n\n        <label htmlFor='piece02'>Piece</label>\n        <input type='text' name='piece02' id={2} value={ pie.piece02 } onChange={ setPiece }/>\n\n        <label htmlFor='piece03'>Piece</label>\n        <input type='text' name='piece03' id={3} value={ pie.piece03 } onChange={ setPiece }/>\n\n        <label htmlFor='piece04'>Piece</label>\n        <input type='text' name='piece04' id={4} value={ pie.piece04 } onChange={ setPiece }/>\n\n        <label htmlFor='piece05'>Piece</label>\n        <input type='text' name='piece05' id={5} value={ pie.piece05 } onChange={ setPiece }/>\n        \n        <label htmlFor='piece06'>Piece</label>\n        <input type='text' name='piece06' id={6} value={ pie.piece06 } onChange={ setPiece }/>\n\n        <label htmlFor='piece07'>Piece</label>\n        <input type='text' name='piece07' id={7} value={ pie.piece07 } onChange={ setPiece }/>\n\n        <input type='submit' value=\"Save\" onClick={(event) => {\n          event.preventDefault()\n          if(pie.title.isEmpty() || pie.pieces[0].isEmpty() || pie.pieces[1].isEmpty() || pie.pieces[2].isEmpty() || pie.pieces[3].isEmpty() || pie.pieces[4].isEmpty() || pie.pieces[5].isEmpty() || pie.pieces[6].isEmpty() || pie.pieces[7].isEmpty()) {\n            alert('All fields are required')\n          }\n          else {\n            pie.user_id = user.id\n            console.log('pie', pie)\n            createPie(pie, history, user)\n          }\n        }} />\n      </form>\n    </div>\n  )\n}\n\nPieForm.propTypes = {\n  createPie: PropTypes.func.isRequired\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    user: state.authenticationReducer.user\n  }\n}\n\nexport default connect(mapStateToProps, { createPie })(PieForm)\n","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/Pie.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/actions/pieActions.js",["97"],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/actions/types.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/actions/urlPrefix.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/store.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/reducers/index.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/reducers/pieReducer.js",["98"],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PrivateRoute.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/LoginForm.js",["99"],"import React, { Component, useState, useEffect } from 'react'\nimport { connect } from 'react-redux'\nimport { login } from '../actions/userActions'\nimport { useHistory } from \"react-router\";\nimport PropTypes from 'prop-types'\n\nexport const LoginForm = ({\n  login,\n  errorMessage\n}) => {\n\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n  const [loginButtonDisabled, setLoginButtonDisabled] = useState(true)\n  const history = useHistory()\n\n  useEffect(() => {\n    setLoginButtonDisabled(!(email && password))\n  }, [email, password])\n\n  const handleChange = (event) => {\n    \n    if(event.target.name === 'email') {\n      setEmail(event.target.value)\n    }\n    else if (event.target.name === 'password') {\n      setPassword(event.target.value)\n    }\n  }\n\n  const submit = (event) => {\n    event.preventDefault()\n    login(email, password, history)\n  }\n\n  return (\n    <div>\n      <h1>Log in</h1>\n      <div className=\"error\">{ errorMessage }</div>\n      <form className='form'>\n        <label htmlFor='email'>Email</label>\n        <input type='text' name='email' value={ email } onChange={ handleChange } />\n\n        <label htmlFor='password'>Password</label>\n        <input type='password' name='password' value={ password } onChange={ handleChange } />\n\n        <input type='submit' value=\"Log in\" disabled={ loginButtonDisabled } onClick={ submit } />\n      </form>\n    </div>\n  )\n}\n\nLoginForm.propTypes = {\n  login: PropTypes.func.isRequired\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    errorMessage: state.authenticationReducer.errorMessage\n  }\n}\n\nexport default connect(mapStateToProps, { login })(LoginForm)\n","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/reducers/authenticationReducer.js",["100"],"import { SIGNUP_REQUEST, SIGNUP_SUCCESS, SIGNUP_FAILURE, LOGIN_REQUEST, LOGIN_SUCCESS, LOGIN_FAILURE, LOGOUT } from '../actions/types';\n\nconst initialState = {\n  loggedIn: false,\n  user: {}\n}\n\nexport default (state = initialState, action) => {\n  switch (action.type) {\n    case LOGIN_REQUEST:\n      return {\n        user: action.user\n      }\n    case LOGIN_SUCCESS:\n      return {\n        loggedIn: true,\n        user: action.user\n      }\n    case LOGIN_FAILURE:\n      return {\n        loggedIn: false,\n        errorMessage: action.error\n      }\n    case LOGOUT:\n      return {\n        loggedIn: false,\n        user: {}\n      }\n    case SIGNUP_REQUEST:\n      return {\n        user: action.user\n      }\n    case SIGNUP_SUCCESS:\n      return {\n        loggedIn: true,\n        user: action.user\n      }\n    case SIGNUP_FAILURE:\n      return {\n        loggedIn: false,\n        errorMessage: action.error\n      }\n    default:\n      return state\n  }\n}","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/actions/userActions.js",["101"],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/SignupForm.js",["102"],"import React, { Component, useState, useEffect } from 'react'\nimport { connect } from 'react-redux'\nimport { signup } from '../actions/userActions'\nimport { useHistory } from \"react-router\";\nimport PropTypes from 'prop-types'\n\nexport const SignupForm = ({\n  signup,\n  errorMessage\n}) => {\n\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n  const [firstname, setFirstname] = useState('')\n  const [lastname, setLastname] = useState('')\n  const [signupButtonDisabled, setSignupButtonDisabled] = useState(true)\n  const history = useHistory()\n\n  useEffect(() => {\n    setSignupButtonDisabled(!(email && password && firstname))\n  }, [email, password, firstname, lastname])\n\n  const handleChange = (event) => {\n    \n    let theFunc\n\n    if(event.target.name === 'email')           theFunc = setEmail\n    else if (event.target.name === 'password')  theFunc = setPassword\n    else if (event.target.name === 'firstname') theFunc = setFirstname\n    else if (event.target.name === 'lastname')  theFunc = setLastname\n\n    theFunc(event.target.value)\n  }\n\n  const submit = (event) => {\n    event.preventDefault()\n    signup(email, password, firstname, lastname, history)\n  }\n\n  return (\n    <div>\n      <h1>Sign up</h1>\n      <div className=\"error\">{ errorMessage }</div>\n      <form className='form'>\n        \n        <label htmlFor='firstname'>First name (required)</label>\n        <input type='text' name='firstname' value={ firstname } onChange={ handleChange } />\n\n        <label htmlFor='lastname'>Last name</label>\n        <input type='text' name='lastname' value={ lastname } onChange={ handleChange } />\n\n        <label htmlFor='email'>Email (required)</label>\n        <input type='text' name='email' value={ email } onChange={ handleChange } />\n\n        <label htmlFor='password'>Password (required)</label>\n        <input type='password' name='password' value={ password } onChange={ handleChange } />\n\n        <input type='submit' value=\"Sign up\" disabled={ signupButtonDisabled } onClick={ submit } />\n      </form>\n    </div>\n  )\n}\n\nSignupForm.propTypes = {\n  signup: PropTypes.func.isRequired\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    errorMessage: state.authenticationReducer.errorMessage\n  }\n}\n\nexport default connect(mapStateToProps, { signup })(SignupForm)\n","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PieGallery.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PieControls.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PieController.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/Navigation.js",[],"/home/geodevadmin/programming