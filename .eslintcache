},{"filePath":"91","messages":"92","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"93","messages":"94","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"46"},{"filePath":"95","messages":"96","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/index.js",[],["97","98"],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/App.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PieForm.js",["99"],"import React, { useState } from \"react\"\n\nimport PropTypes from 'prop-types'\nimport { connect } from 'react-redux'\nimport { createPie } from '../actions/pieActions'\n\nexport const PieForm = ({\n  createPie,\n  history\n}) => {\n\n  const [pie, setPie] = useState({\n    title: '',\n    pieces: ['', '', '', '', '', '', '', '']\n  })\n  \n  const setTitle = (event) => {\n    setPie({\n      ...pie,\n      title: event.target.value\n    })\n  }\n\n  const setPiece = (event) => {\n    const p = [...pie.pieces]\n    p[event.target.id] = event.target.value\n    setPie({\n      ...pie,\n      pieces: p\n    })\n  }\n\n  String.prototype.isEmpty = function() {\n    return (this.length === 0 || !this.trim())\n  }\n\n  return (\n    <div>\n      <h1>Create Pie</h1>\n      <form className='form'>\n        <label htmlFor='title'>Title</label>\n        <input type='text' name='title' value={ pie.title } onChange={ setTitle } />\n\n        <label htmlFor='piece00'>Piece</label>\n        <input type='text' name='piece00' id={0} value={ pie.piece00 } onChange={ setPiece } />\n\n        <label htmlFor='piece01'>Piece</label>\n        <input type='text' name='piece01' id={1} value={ pie.piece01 } onChange={ setPiece }/>\n\n        <label htmlFor='piece02'>Piece</label>\n        <input type='text' name='piece02' id={2} value={ pie.piece02 } onChange={ setPiece }/>\n\n        <label htmlFor='piece03'>Piece</label>\n        <input type='text' name='piece03' id={3} value={ pie.piece03 } onChange={ setPiece }/>\n\n        <label htmlFor='piece04'>Piece</label>\n        <input type='text' name='piece04' id={4} value={ pie.piece04 } onChange={ setPiece }/>\n\n        <label htmlFor='piece05'>Piece</label>\n        <input type='text' name='piece05' id={5} value={ pie.piece05 } onChange={ setPiece }/>\n        \n        <label htmlFor='piece06'>Piece</label>\n        <input type='text' name='piece06' id={6} value={ pie.piece06 } onChange={ setPiece }/>\n\n        <label htmlFor='piece07'>Piece</label>\n        <input type='text' name='piece07' id={7} value={ pie.piece07 } onChange={ setPiece }/>\n\n        <input type='submit' value=\"Save\" onClick={(event) => {\n          event.preventDefault()\n          if(pie.title.isEmpty() || pie.pieces[0].isEmpty() || pie.pieces[1].isEmpty() || pie.pieces[2].isEmpty() || pie.pieces[3].isEmpty() || pie.pieces[4].isEmpty() || pie.pieces[5].isEmpty() || pie.pieces[6].isEmpty() || pie.pieces[7].isEmpty()) {\n            alert('All fields are required')\n          }\n          else {\n            createPie(pie, history)\n          }\n        }} />\n      </form>\n    </div>\n  )\n}\n\nPieForm.propTypes = {\n  createPie: PropTypes.func.isRequired\n}\n\nexport default connect(null, { createPie })(PieForm)\n","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/Pie.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/actions/pieActions.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/actions/types.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/actions/urlPrefix.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/store.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/reducers/index.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/reducers/pieReducer.js",["100"],"import { CREATE_PIE, FETCH_PIE, UPDATE_PIE, FETCH_PIES } from \"../actions/types\";\n\nconst initialState = {\n  pie: {},\n  pies: []\n}\n\nexport default (state = initialState, action) => {\n  switch(action.type) {\n    case FETCH_PIE:\n      return {\n        ...state,\n        pie: action.payload\n      };\n    case UPDATE_PIE:\n      return {\n        ...state,\n        pie: action.payload\n      };\n    case CREATE_PIE:\n      return {\n        ...state,\n        pie: action.payload\n      };\n    case FETCH_PIES:\n      return {\n        ...state,\n        pies: action.payload\n      };\n    default:\n      return state\n  }\n}",["101","102"],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PrivateRoute.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/LoginForm.js",["103"],"import React, { Component, useState, useEffect } from 'react'\nimport { connect } from 'react-redux'\nimport { login } from '../actions/userActions'\nimport { useHistory } from \"react-router\";\nimport PropTypes from 'prop-types'\n\nexport const LoginForm = ({\n  login\n}) => {\n\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n  const [loginButtonDisabled, setLoginButtonDisabled] = useState(true)\n  const history = useHistory()\n\n  useEffect(() => {\n    setLoginButtonDisabled(!(email && password))\n  }, [email, password])\n\n  const handleChange = (event) => {\n    \n    if(event.target.name === 'email') {\n      setEmail(event.target.value)\n    }\n    else if (event.target.name === 'password') {\n      setPassword(event.target.value)\n    }\n  }\n\n  const submit = (event) => {\n    event.preventDefault()\n    login(email, password, history)\n  }\n\n  return (\n    <div>\n      <h1>Log in</h1>\n      <form className='form'>\n        <label htmlFor='email'>Email</label>\n        <input type='text' name='email' value={ email } onChange={ handleChange } />\n\n        <label htmlFor='password'>Password</label>\n        <input type='password' name='password' value={ password } onChange={ handleChange } />\n\n        <input type='submit' value=\"Log in\" disabled={ loginButtonDisabled } onClick={ submit } />\n      </form>\n    </div>\n  )\n}\n\nLoginForm.propTypes = {\n  login: PropTypes.func.isRequired\n}\n\nexport default connect(null, { login })(LoginForm)\n","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/reducers/authenticationReducer.js",["104"],"import { SIGNUP_REQUEST, SIGNUP_SUCCESS, SIGNUP_FAILURE, LOGIN_REQUEST, LOGIN_SUCCESS, LOGIN_FAILURE, LOGOUT } from '../actions/types';\n\nconst initialState = {\n  loggedIn: false,\n  user: {}\n}\n\nexport default (state = initialState, action) => {\n  switch (action.type) {\n    case LOGIN_REQUEST:\n      return {\n        loggingIn: true,\n        user: action.user\n      };\n    case LOGIN_SUCCESS:\n      return {\n        loggedIn: true,\n        user: action.user\n      };\n    case LOGIN_FAILURE:\n      return {\n        errorMessage: action.error\n      };\n    case LOGOUT:\n      return {};\n    case SIGNUP_REQUEST:\n      return {\n        loggingIn: true,\n        user: action.user\n      };\n    case SIGNUP_SUCCESS:\n      return {\n        loggedIn: true,\n        user: action.user\n      };\n    case SIGNUP_FAILURE:\n      return {\n        errorMessage: action.error\n      };\n    default:\n      return state\n  }\n}","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/MyPies.js",["105"],"import React from 'react'\nimport PropTypes from 'prop-types'\n\nfunction MyPies(props) {\n  return (\n    <div>My Pies</div>\n  )\n}\n\nMyPies.propTypes = {\n\n}\n\nexport default MyPies\n\n","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/Home.js",["106"],"import React from 'react'\nimport PropTypes from 'prop-types'\n\nfunction Home(props) {\n  return (\n    <div>Home</div>\n  )\n}\n\nHome.propTypes = {\n\n}\n\nexport default Home\n\n",["107","108"],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/actions/userActions.js",["109"],"import { SIGNUP_REQUEST, SIGNUP_SUCCESS, SIGNUP_FAILURE, LOGIN_REQUEST, LOGIN_SUCCESS, LOGIN_FAILURE, LOGOUT } from './types'\nimport { URL_PREFIX } from './urlPrefix'\n\nString.prototype.capitalize = function() {\n  return this.charAt(0).toUpperCase() + this.slice(1)\n}\n\nconst getErrorMessage = ( user ) => {\n  \n  let errorMessage = ''\n  \n  if(user.errors) {\n    errorMessage = Object.keys(user.errors).map((key) => `${key.capitalize()} ${[...new Set(user.errors[key])].join(', ')}`).join(', ') + '.'\n  }\n  else if(user.error) {\n    errorMessage = user.error\n  }\n  \n  return errorMessage\n}\n\nexport const login = (email, password, history) => {\n  \n  return dispatch => {\n\n    dispatch({ type: LOGIN_REQUEST, email })\n\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\"user\":{\"email\":email, \"password\":password}})\n    }\n\n    fetch(`${ URL_PREFIX }/api/users/login`, requestOptions)\n      .then(response => response.json())\n      .then(user => {\n        \n        if(user.error || user.errors) {\n          dispatch({ \n            type: LOGIN_FAILURE,\n            error: getErrorMessage(user)\n          })\n        }\n        else {\n          dispatch({\n            type: LOGIN_SUCCESS,\n            user: user\n          })\n          history.push('/mypies', null)\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: LOGIN_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}\n\nexport const logout = () => {\n  return { type: LOGOUT }\n}\n\nexport const signup = (email, password, firstname, lastname, history) => {\n  \n  return dispatch => {\n\n    dispatch({ type: SIGNUP_REQUEST, email })\n\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\"user\":{\"email\":email, \"password\":password, \"first_name\": firstname, \"last_name\": lastname}})\n    }\n\n    fetch(`${ URL_PREFIX }/api/users`, requestOptions)\n      .then(response => response.json())\n      .then(user => {\n        if(user.error || user.errors) {\n          dispatch({ \n            type: SIGNUP_FAILURE,\n            error: getErrorMessage(user)\n          })\n        }\n        else {\n          dispatch({\n            type: SIGNUP_SUCCESS,\n            user: user\n          })\n          history.push('/mypies', null)\n        }\n      })\n      .catch((error) => {\n        dispatch({ \n          type: SIGNUP_FAILURE,\n          error: error.toString()\n        })\n      })\n    }\n}","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/SignupForm.js",["110"],"import React, { Component, useState, useEffect } from 'react'\nimport { connect } from 'react-redux'\nimport { signup } from '../actions/userActions'\nimport { useHistory } from \"react-router\";\nimport PropTypes from 'prop-types'\n\nexport const SignupForm = ({\n  signup\n}) => {\n\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n  const [firstname, setFirstname] = useState('')\n  const [lastname, setLastname] = useState('')\n  const [signupButtonDisabled, setSignupButtonDisabled] = useState(true)\n  const history = useHistory()\n\n  useEffect(() => {\n    setSignupButtonDisabled(!(email && password && firstname))\n  }, [email, password, firstname, lastname])\n\n  const handleChange = (event) => {\n    \n    let theFunc\n\n    if(event.target.name === 'email')           theFunc = setEmail\n    else if (event.target.name === 'password')  theFunc = setPassword\n    else if (event.target.name === 'firstname') theFunc = setFirstname\n    else if (event.target.name === 'lastname')  theFunc = setLastname\n\n    theFunc(event.target.value)\n  }\n\n  const submit = (event) => {\n    event.preventDefault()\n    signup(email, password, firstname, lastname, history)\n  }\n\n  return (\n    <div>\n      <h1>Sign up</h1>\n      <form className='form'>\n        \n        <label htmlFor='firstname'>First name (required)</label>\n        <input type='text' name='firstname' value={ firstname } onChange={ handleChange } />\n\n        <label htmlFor='lastname'>Last name</label>\n        <input type='text' name='lastname' value={ lastname } onChange={ handleChange } />\n\n        <label htmlFor='email'>Email (required)</label>\n        <input type='text' name='email' value={ email } onChange={ handleChange } />\n\n        <label htmlFor='password'>Password (required)</label>\n        <input type='password' name='password' value={ password } onChange={ handleChange } />\n\n        <input type='submit' value=\"Sign up\" disabled={ signupButtonDisabled } onClick={ submit } />\n      </form>\n    </div>\n  )\n}\n\nSignupForm.propTypes = {\n  signup: PropTypes.func.isRequired\n}\n\nexport default connect(null, { signup })(SignupForm)\n","/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PieGallery.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PieControls.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/PieController.js",[],"/home/geodevadmin/programming/node_apps/user-auth-jwt-redux/src/components/Navigation.js",[],{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":33,"column":3,"nodeType":"117","messageId":"118","endLine":35,"endColumn":4},{"ruleId":"119","severity":1,"message":"120","line":8,"column":1,"nodeType":"121","endLine":33,"endColumn":2},{"ruleId":"111","replacedBy":"122"},{"ruleId":"113","replacedBy":"123"},{"ruleId":"124","severity":1,"message":"125","line":1,"column":17,"nodeType":"126","messageId":"127","endLine":1,"endColumn":26},{"ruleId":"119","severity":1,"message":"12